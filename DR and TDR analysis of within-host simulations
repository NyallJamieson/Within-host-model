low_dist_gamma <- fitdistrplus::fitdist(low/24,"gamma",method="mle")
low_dist_weibull <- fitdistrplus::fitdist(low/24,"weibull",method="mle")
low_dist_burr <- fitdistrplus::fitdist(low/24,"burrNEW",method="mle",start=list(a=1,b=1,T=5))
med_dist_norm <- fitdistrplus::fitdist(medium/24,"norm",method="mle")
med_dist_lnorm <- fitdistrplus::fitdist(medium/24,"lnorm",method="mle")
med_dist_gamma <- fitdistrplus::fitdist(medium/24,"gamma",method="mle")
med_dist_weibull <- fitdistrplus::fitdist(medium/24,"weibull",method="mle")
med_dist_burr <- fitdistrplus::fitdist(medium/24,"burrNEW",method="mle",start=list(a=1,b=1,T=5))
high_dist_norm <- fitdistrplus::fitdist(high/24,"norm",method="mle")
high_dist_lnorm <- fitdistrplus::fitdist(high/24,"lnorm",method="mle")
high_dist_gamma <- fitdistrplus::fitdist(high/24,"gamma",method="mle")
high_dist_weibull <- fitdistrplus::fitdist(high/24,"weibull",method="mle")
high_dist_burr <- fitdistrplus::fitdist(high/24,"burrNEW",method="mle",start=list(a=0.5,b=0.5,T=5))
