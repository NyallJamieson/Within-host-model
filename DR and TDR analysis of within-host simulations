# Function to compute WCSS
calculate_wcss <- function(k) {
  clusters_k <- cutree(hc, k)
  clusters_list <- split(summary_matrix, clusters_k)
  wcss <- sum(sapply(clusters_list, function(cluster) {
    sum(dist(cluster)^2)
  }))
  return(wcss)
}

